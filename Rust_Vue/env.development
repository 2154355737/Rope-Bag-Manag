# 开发环境配置
# 文件名: .env.development
# 此文件在开发模式下自动加载

# API基础URL - 开发环境使用相对路径，通过Vite代理
# 使用代码中的默认值 /api，不在环境变量中重复设置
# VITE_API_BASE_URL=/api

# 可选：直接指向后端服务器（跳过代理）
# VITE_API_BASE_URL=http://127.0.0.1:15201/api

# 应用配置
VITE_APP_TITLE=绳包管理器 - 开发环境
VITE_APP_VERSION=1.0.0
VITE_ENV=development

# 调试配置
VITE_DEBUG_API=true 